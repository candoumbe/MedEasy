{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ApiOptions": {
    "MaxPageSize": 50
  },
  "Swagger": {
    "Contact": {
      "Email": "ndoumbecyrille@hotmail.com",
      "Name": "Cyrille-Alexandre NDOUMBE"
    }
  },
  "ConsulConfig": {
    /* Consul server address */
    "Address": "http://127.0.0.1:8500",
    "ServiceName": "Agenda",
    "Tags": [ "agenda", "appointments", "participants" ],
    "Check": {
      "IntervalInSecond": 10,
      "TimeoutInSecond": 30,
      "Endpoint": "agenda/health/status"
    }
  }
}
