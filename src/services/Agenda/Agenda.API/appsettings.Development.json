{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConsulConfig": {
    /* Consul server address */
    "Address": "http://127.0.0.1:8500",
    "ServiceName": "Agenda",
    "Tags": [ "agenda", "appointments", "participants" ],
    "Check": {
      "IntervalInSecond": 10,
      "TimeoutInSecond": 30,
      "Endpoint" : "agenda/health/status"
    }
  }

  
}
